{
  "name": "thefoovault/sf-skeleton",
  "autoload": {
    "psr-4": {
      "Shared\\": "src/Shared",
      "BookCatalog\\": "src/BookCatalog",
      "BookCatalogAPI\\": "apps/BookCatalogAPI/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "extra": {
    "bin-dir": "apps/BookCatalogAPI/bin",
    "config-dir": "apps/BookCatalogAPI/config",
    "src-dir": "apps/BookCatalogAPI/src",
    "var-dir": "apps/BookCatalogAPI/var",
    "public-dir": "apps/BookCatalogAPI/public"
  },
  "require": {
    "symfony/flex": "^1.13",
    "symfony/framework-bundle": "^5.3",
    "symfony/dotenv": "^5.3",
    "symfony/yaml": "^5.3",
    "symfony/console": "^5.3",
    "symfony/runtime": "^5.3",
    "symfony/messenger": "^5.3",
    "lambdish/phunctional": "^2.1",
    "composer/package-versions-deprecated": "1.11.99.2",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.9",
    "symfony/proxy-manager-bridge": "^5.3"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "dg/bypass-finals": "^1.3"
  }
}
